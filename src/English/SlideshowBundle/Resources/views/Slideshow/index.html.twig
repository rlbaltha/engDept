 {% extends 'EnglishSlideshowBundle:Slideshow:layout.html.twig' %}
 
 {% block body %}

<div class="row"><div class="cell"><a href="{{ path('slideshow_new') }}">New</a></div></div>

    {% for entity in entities %}
        <div class="row">
            <div class="cell"><a href="{{ path('slideshow_show', { 'id': entity.id }) }}">{{ entity.id }}</a></div>
            <div class="cell">{{ entity.photoUrl }}</div>
            <div class="cell">{{ entity.title }}</div>
            <div class="cell">{{ entity.type }}</div>
            <div class="cell">
                <a href="{{ path('slideshow_show', { 'id': entity.id }) }}">show</a>
                <a href="{{ path('slideshow_edit', { 'id': entity.id }) }}">edit</a>
            </div>
        </div>
    {% endfor %}
    


{% endblock %}
