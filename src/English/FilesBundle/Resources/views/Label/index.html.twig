{% extends 'EnglishFilesBundle:File:layout.html.twig' %}
 
{% block body %}

<div class="row divider"><div class="cell"><a href="{{ path('label_new') }}">Add Label</a></div></div>

{% for entity in entities %}
    <div class='row divider'>
        <div class="cell">{{ entity.name }}</div>
        <div class="cell">{{ entity.display }}</div>
        <div class="cell"><a href="{{ path('label_edit', { 'id': entity.id }) }}">Edit</a></div>
        
        
    </div>

    {% endfor %}

{% endblock %}
