 {% extends 'EnglishAdvisorsBundle:Advisor:layout.html.twig' %}
 
 {% block body %}

 
        <div class="row">
            <div class="cell">Id</div>
            <div class="cell">{{ entity.id }}</div>
        </div>
        <div class="row">
            <div class="cell">Name</div>
            <div class="cell">{{ entity.name }}</div>
        </div>
        <div class="row">
            <div class="cell">Username</div>
            <div class="cell">{{ entity.username }}</div>
        </div>
    



    
        <a href="{{ path('advisor') }}">
            Back to the list
        </a>
    
    
        <a href="{{ path('advisor_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    
    
        <form action="{{ path('advisor_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    

{% endblock %}
