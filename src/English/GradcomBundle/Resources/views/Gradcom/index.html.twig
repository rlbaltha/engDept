 {% extends 'EnglishGradcomBundle:Gradcom:layout.html.twig' %}
 
 {% block body %}

<div class="row"><div class="cell"><a href="{{ path('gradcom_new') }}">New</a></div></div>
    
    {% for entity in entities %}
        <div class="row">
            <div class="cell"><a href="{{ path('gradcom_show', { 'id': entity.id }) }}">{{ entity.id }}</a></div>
            <div class="cell">{{ entity.gid }}</div>
            <div class="cell">{{ entity.fid }}</div>
            <div class="cell">{{ entity.frole }}</div>
            <div class="cell">{{ entity.status }}</div>
            <div class="cell">
                <a href="{{ path('gradcom_show', { 'id': entity.id }) }}">show</a>
                <a href="{{ path('gradcom_edit', { 'id': entity.id }) }}">edit</a>  
            </div>
        </div>
    {% endfor %}

{% endblock %}
