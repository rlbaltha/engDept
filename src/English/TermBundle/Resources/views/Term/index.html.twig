 {% extends 'EnglishTermBundle:Term:layout.html.twig' %}
 
 {% block body %}

<div class="row"><div class="cell"><a href="{{ path('term_new') }}">New</a></div></div>
   
    {% for entity in entities %}
        <div class="row">
            <div class="cell"><a href="{{ path('term_show', { 'id': entity.id }) }}">{{ entity.id }}</a></div>
            <div class="cell">{{ entity.term }}</div>
            <div class="cell">{{ entity.termName }}</div>
            <div class="cell">{{ entity.type }}</div>
            <div class="cell">
                <a href="{{ path('term_show', { 'id': entity.id }) }}">show</a>
                <a href="{{ path('term_edit', { 'id': entity.id }) }}">edit</a>
            </div>
        </div>
    {% endfor %}

{% endblock %}
