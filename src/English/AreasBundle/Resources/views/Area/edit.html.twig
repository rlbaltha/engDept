{% extends 'EnglishAreasBundle:Area:layout.html.twig' %}

{% block body %}

<div class="row divider">
    <div class="cell"><a href="{{ path('area') }}">List Areas</a></div>   
</div>

<div class="row">
    <div class="cell">
        <form action="{{ path('area_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
        {{ form_widget(edit_form) }}
        <button type="submit">Update</button>
        </form>
    </div>    
</div>



{% endblock %}
