 {% extends 'EnglishCoursesBundle:Course:layout.html.twig' %}
 
 {% block body %}

        <div class="row">
            <div class="cell">Id</div>
            <div class="cell">{{ entity.id }}</div>
        </div>
        <div class="row">
            <div class="cell">Coursename</div>
            <div class="cell">{{ entity.courseName }}</div>
        </div>
        <div class="row">
            <div class="cell">Instructorname</div>
            <div class="cell">{{ entity.instructorName }}</div>
        </div>
        <div class="row">
            <div class="cell">Callnumber</div>
            <div class="cell">{{ entity.callNumber }}</div>
        </div>
        <div class="row">
            <div class="cell">Room</div>
            <div class="cell">{{ entity.room }}</div>
        </div>
        <div class="row">
            <div class="cell">Days</div>
            <div class="cell">{{ entity.days }}</div>
        </div>
        <div class="row">
            <div class="cell">Term</div>
            <div class="cell">{{ entity.term }}</div>
        </div>
        <div class="row">
            <div class="cell">Time</div>
            <div class="cell">{{ entity.time }}</div>
        </div>
        <div class="row">
            <div class="cell">Building</div>
            <div class="cell">{{ entity.building }}</div>
        </div>
        <div class="row">
            <div class="cell">Callnumber2</div>
            <div class="cell">{{ entity.callNumber2 }}</div>
        </div>
        <div class="row">
            <div class="cell">Username</div>
            <div class="cell">{{ entity.username }}</div>
        </div>
        <div class="row">
            <div class="cell">Period</div>
            <div class="cell">{{ entity.period }}</div>
        </div>
        <div class="row">
            <div class="cell">Title</div>
            <div class="cell">{{ entity.title }}</div>
        </div>
        <div class="row">
            <div class="cell">Area</div>
            <div class="cell">{{ entity.area }}</div>
        </div>
        <div class="row">
            <div class="cell">May</div>
            <div class="cell">{{ entity.may }}</div>
        </div>
    



    
        <a href="{{ path('course') }}">
            Back to the list
        </a>
    
    
        <a href="{{ path('course_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    
    
        <form action="{{ path('course_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    

{% endblock %}
