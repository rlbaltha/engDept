 {% extends 'EnglishMajorsBundle:Major:layout.html.twig' %}
 
 {% block body %}

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Advisor</th>
            <td>{{ entity.advisor }}</td>
        </tr>
        <tr>
            <th>Mentorusername</th>
            <td>{{ entity.mentorUserName }}</td>
        </tr>
        <tr>
            <th>Firstmajor</th>
            <td>{{ entity.firstMajor }}</td>
        </tr>
        <tr>
            <th>Secondmajor</th>
            <td>{{ entity.secondMajor }}</td>
        </tr>
        <tr>
            <th>Aoe</th>
            <td>{{ entity.aoe }}</td>
        </tr>
        <tr>
            <th>Can</th>
            <td>{{ entity.can }}</td>
        </tr>
        <tr>
            <th>Minor</th>
            <td>{{ entity.minor }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ entity.status }}</td>
        </tr>
        <tr>
            <th>Notes</th>
            <td>{{ entity.notes }}</td>
        </tr>
        <tr>
            <th>Honors</th>
            <td>{{ entity.honors }}</td>
        </tr>
        <tr>
            <th>Advisorid</th>
            <td>{{ entity.advisorId }}</td>
        </tr>
        <tr>
            <th>Mentorid</th>
            <td>{{ entity.mentorId }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('major') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('major_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('major_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
