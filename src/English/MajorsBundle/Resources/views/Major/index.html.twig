 {% extends 'EnglishMajorsBundle:Major:layout.html.twig' %}
 
 {% block body %}
<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Advisor</th>
            <th>Mentorusername</th>
            <th>Firstmajor</th>
            <th>Secondmajor</th>
            <th>Aoe</th>
            <th>Can</th>
            <th>Minor</th>
            <th>Status</th>
            <th>Notes</th>
            <th>Honors</th>
            <th>Advisorid</th>
            <th>Mentorid</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('major_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.name }}</td>
            <td>{{ entity.email }}</td>
            <td>{{ entity.advisor }}</td>
            <td>{{ entity.mentorUserName }}</td>
            <td>{{ entity.firstMajor }}</td>
            <td>{{ entity.secondMajor }}</td>
            <td>{{ entity.aoe }}</td>
            <td>{{ entity.can }}</td>
            <td>{{ entity.minor }}</td>
            <td>{{ entity.status }}</td>
            <td>{{ entity.notes }}</td>
            <td>{{ entity.honors }}</td>
            <td>{{ entity.advisorId }}</td>
            <td>{{ entity.mentorId }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('major_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('major_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('major_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
{% endblock %}
